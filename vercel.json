{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist/public",
  "installCommand": "npm ci",
  "routes": [
    {
      "src": "/api/test",
      "dest": "/api/test.js"
    },
    {
      "src": "/api/simple-test",
      "dest": "/api/simple-test.js"
    },
    {
      "src": "/api/test-session",
      "dest": "/api/test-session.js"
    },
    {
      "src": "/api/test-db-connection",
      "dest": "/api/test-db-connection.js"
    },
    {
      "src": "/api/setup-db",
      "dest": "/api/setup-db.js"
    },
    {
      "src": "/api/import-users",
      "dest": "/api/import-users.js"
    },
    {
      "src": "/api/debug-login",
      "dest": "/api/debug-login.js"
    },
    {
      "src": "/api/inspect-db",
      "dest": "/api/inspect-db.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/dist/public/assets/$1",
      "headers": {
        "Access-Control-Allow-Origin": "*"
      }
    },
    {
      "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot))",
      "dest": "/dist/public/$1",
      "headers": {
        "Access-Control-Allow-Origin": "*"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}